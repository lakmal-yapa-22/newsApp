{
  "expo": {
    "name": "NewsLite",
    "slug": "NewsLite",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/news.png",
    "scheme": "news-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/news.png",
        "backgroundImage": "./assets/images/news.png",
        "monochromeImage": "./assets/images/news.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.lakmalkumarasiri.newsapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/news.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7f2104b9-c289-40b2-9d4d-44dab7b416d1"
      }
    }
  }
}
